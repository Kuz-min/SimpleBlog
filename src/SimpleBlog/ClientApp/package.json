{
  "name": "simpleblog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44468 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44468 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ssr": "ng run SimpleBlog:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.2",
    "@angular/common": "^15.2.2",
    "@angular/compiler": "^15.2.2",
    "@angular/core": "^15.2.2",
    "@angular/forms": "^15.2.2",
    "@angular/material": "^15.2.2",
    "@angular/platform-browser": "^15.2.2",
    "@angular/platform-browser-dynamic": "^15.2.2",
    "@angular/platform-server": "^15.2.2",
    "@angular/router": "^15.2.2",
    "@ngneat/elf": "^2.3.1",
    "@ngneat/elf-entities": "^4.4.4",
    "@ngneat/elf-requests": "^1.6.0",
    "angular-oauth2-oidc": "^15.0.1",
    "bootstrap": "^5.2.3",
    "material-icons": "^1.13.2",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular/cli": "^15.2.3",
    "@angular/compiler-cli": "^15.2.2",
    "@angular-devkit/build-angular": "^15.2.3",
    "@types/jasmine": "*",
    "@types/jasminewd2": "*",
    "@types/node": "*",
    "jasmine-core": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-coverage": "*",
    "karma-jasmine": "*",
    "karma-jasmine-html-reporter": "*",
    "typescript": "*"
  }
}
